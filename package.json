{
  "name": "bloup",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "c:format": "cd $INIT_CWD && prettier -w .",
    "c:lint": "cd $INIT_CWD && eslint . --cache --cache-location node_modules/.cache/.eslintcache --fix",
    "c:lint:ci": "cd $INIT_CWD && eslint .",
    "c:pkg:deps": "syncpack syncpack lint-semver-ranges",
    "c:pkg:deps:fix": "syncpack fix-mismatches",
    "c:pkg:fix": "yarn c:pkg:deps:fix && yarn c:pkg:format",
    "c:pkg:format": "syncpack format",
    "c:test": "jest $INIT_CWD --cache --passWithNoTests",
    "c:test:ci": "jest $INIT_CWD --passWithNoTests --ci -i",
    "c:test:watch": "jest $INIT_CWD --cache --passWithNoTests --watch",
    "c:type": "cd $INIT_CWD && tsc -b",
    "gen:tsconfig": "yarn node scripts/generators/tsconfig.js"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@graphql-eslint/eslint-plugin": "3.10.2",
    "@jest/types": "27.5.1",
    "@types/glob": "7.2.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-unicorn": "42.0.0",
    "glob": "7.2.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "syncpack": "6.2.0",
    "ts-jest": "27.1.4",
    "tslib": "2.3.1",
    "typescript": "4.6.3"
  }
}
