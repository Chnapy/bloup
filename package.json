{
  "name": "bloup",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@3.2.2",
  "license": "UNLICENSED",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "c:format:fix": "prettier -w $INIT_CWD",
    "c:lint:ci": "eslint $INIT_CWD",
    "c:lint:fix": "eslint $INIT_CWD --cache --cache-location node_modules/.cache/.eslintcache --fix",
    "c:pkg:deps": "syncpack syncpack lint-semver-ranges",
    "c:pkg:deps:fix": "syncpack fix-mismatches -w",
    "c:pkg:fix": "yarn c:pkg:deps:fix && yarn c:pkg:format",
    "c:pkg:format": "syncpack format",
    "c:test": "cd $INIT_CWD && yarn c:test:watch --run",
    "c:test:ci": "cd $INIT_CWD && yarn c:test",
    "c:test:watch": "sh -c \"if [ $INIT_CWD = $PROJECT_CWD ]; then yarn test ${0:-}; else vitest --dir $INIT_CWD --config $INIT_CWD/vite*.config.ts --outputTruncateLength=9999 --passWithNoTests ${0:-}; fi\"",
    "c:type": "cd $INIT_CWD && tsc -b",
    "gen:tsconfig": "ts-node ${PROJECT_CWD}/scripts/generators/tsconfig.ts && prettier -w ${PROJECT_CWD}/**/tsconfig.json",
    "test": "yarn workspaces foreach -v --exclude 'bloup' run c:test ${0:-}"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/eslint-parser": "7.18.9",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@graphql-eslint/eslint-plugin": "3.10.7",
    "@types/glob": "7.2.0",
    "@types/node": "18.7.3",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@vitejs/plugin-react": "2.0.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.4.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "43.0.2",
    "glob": "8.0.3",
    "jsdom": "20.0.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "syncpack": "8.2.4",
    "ts-gql-plugin": "1.3.2",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vite": "3.0.7",
    "vitest": "0.9.4"
  },
  "dependenciesMeta": {
    "esbuild@0.14.54": {
      "built": true
    }
  },
  "os": [
    "linux"
  ]
}
