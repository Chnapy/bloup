{
  "name": "bloup",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "gen:tsconfig": "yarn node scripts/generators/tsconfig.js",
    "c:type": "cd $INIT_CWD && tsc -b",
    "c:lint": "cd $INIT_CWD && eslint . --cache --cache-location node_modules/.cache/.eslintcache --fix",
    "c:format": "cd $INIT_CWD && prettier -w .",
    "c:test": "jest $INIT_CWD --cache --passWithNoTests",
    "c:test:watch": "jest $INIT_CWD --cache --passWithNoTests --watch",
    "c:test:ci": "jest $INIT_CWD --cache --passWithNoTests --ci -i"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@graphql-eslint/eslint-plugin": "3.10.2",
    "@jest/types": "27.5.1",
    "@types/glob": "7.2.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "babel-jest": "27.5.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-unicorn": "42.0.0",
    "glob": "7.2.0",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "ts-jest": "27.0.5",
    "tslib": "2.3.1",
    "typescript": "4.5.2"
  },
  "packageManager": "yarn@3.2.0"
}
